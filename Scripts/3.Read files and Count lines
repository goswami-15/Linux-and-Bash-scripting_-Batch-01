#less - to view a portion of a file
less file_name
#cat - to view files, but best for smaller file
cat file_name
#head -to view from the top of the file
head -5 my_edits.txt
#tail -to view from the bottom of the file
tail -5 my_edits.txt
#wc - for word count
wc my_edits.txt
#wc -l - for line count
wc -l my_edits.txt
#* -  represents zero or more characters
wc -l *.faa
